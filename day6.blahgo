package main

import (
	"fmt"
	"strings"
)

func main() {

	blah := ""
	//blah := "mvwvrwvwbblffmvmhmllmzmqzzbhqnnvrnvnmvvqrqrcc"

	var char, str string
	runes := []rune(blah)

	fmt.Print("first letter: ")
	fmt.Println(string(runes[0:4]))

	fmt.Println("start loop")
	for i, b := range runes {

		fmt.Print(i)
		fmt.Print(": ")
		fmt.Print(string(b))
		char = string(b)
		fmt.Print(": ")
		fmt.Println(string(runes[(i + 1):(i + 4)]))
		str = string(runes[(i + 1):(i + 4)])
		fmt.Println(strings.Contains(str, char))

		if !(strings.Contains(str, char)) {

			// get the first char in str and compare to the other two
			lvl2char := string(runes[(i + 1):(i + 2)])
			lvl2str := string(runes[(i + 2):(i + 4)])
			fmt.Println(lvl2char)
			fmt.Println(lvl2str)

			if !(strings.Contains(lvl2str, lvl2char)) {

				lvl3char := string(runes[(i + 2):(i + 3)])
				lvl3str := string(runes[(i + 3):(i + 4)])
				fmt.Println(lvl3char)
				fmt.Println(lvl3str)

				if !(strings.Contains(lvl3str, lvl3char)) {
					fmt.Println(i + 4)
					break
				}
			}

		}

	}

}
